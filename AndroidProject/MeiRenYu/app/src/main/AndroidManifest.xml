<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yss.yumeiren">
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GoodsDetailaActivity"/>//购物详情页面
        <activity android:name=".SettingActivity"/>//设置页面
        <activity android:name=".ServicerActivity"/>//客服页面
        <activity android:name=".GoodsSearchActivity"/>商品搜索页面
        <activity android:name=".MessageTiShiActivity"/>消息提示页面
        <activity android:name="meirenyu.Login"/><!--我的页面-->
        <activity android:name="meirenyu.MyFriends"/><!--我的好友页-->
        <activity android:name="meirenyu.MyPlanDetail"/><!--我的计划详情页-->
        <activity android:name="meirenyu.MyPlanList"/><!--我的计划列表页-->
        <activity android:name="meirenyu.SetLogin"/><!--登录页面-->
        <activity android:name="meirenyu.SetWeibo"/><!--绑定微博-->
        <activity android:name="meirenyu.SetRegister"/><!--注册页面-->
        <activity android:name="meirenyu.ShareApp"/><!--软件分享页面-->
        <activity android:name="meirenyu.AboutUs"/><!--关于我们页面-->
        <activity android:name="meirenyu.ChatPage"/><!--好友聊天页面-->
        <activity android:name="meirenyu.HaveLoggedIn"/><!--登录完成页面-->

    </application>
</manifest>